{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["dom", "esnext"],
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "./dist/",
    "target": "ES6",
    "noPropertyAccessFromIndexSignature": false,
    "plugins": [{
      "name": "typescript-plugin-css-modules",
      "options": {
        "customMatcher": "\\.css$"
      }
    }]
  }
}
